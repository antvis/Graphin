{
  "name": "@antv/graphin",
  "version": "3.0.0-beta.1",
  "description": "A React toolkit for graph analysis based on g6",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/Graphin.git"
  },
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "run-s clean lib",
    "ci": "run-s type-check lint build",
    "clean": "rimraf lib es",
    "dev": "vite",
    "fix": "eslint ./src --fix && prettier ./src --write ",
    "lib:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "lib:es": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir es",
    "lib": "run-p lib:*",
    "lint": "eslint ./src --quiet && prettier ./src --check",
    "prepublishOnly": "npm run ci",
    "type-check": "tsc --noEmit",
    "watch": "pnpm lib:es --w"
  },
  "dependencies": {
    "@antv/g6": "^5.0.0-beta.39"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.6",
    "antd": "^5.13.2",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "beta",
    "access": "public"
  },
  "license": "MIT"
}
